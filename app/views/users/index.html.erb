<div class="page-header">
	<h1 class="text-info">All Users</h1>
</div>
<table class="table table-striped table-hover">
<thead>
	<tr>
		<th>Email</th>
		<th>Id</th>
	</tr>
</thead>
<tbody>
<% @users.each do |user| %>
  <tr>
	   <td><%= user.email %></td>
	   <td><%= user.id %></td>
  </tr>
</tbody>
<% end %>

</table>
<p><%= link_to "Back to Bookmarks", bookmarks_path %></p>


